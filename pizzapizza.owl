<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.donut.com/ontologies/pizzapizza.owl#"
     xml:base="http://www.donut.com/ontologies/pizzapizza.owl"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:pizzapizza="http://www.pizza.com/ontologies/pizzapizza.owl#">
    <owl:Ontology rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl">
        <rdfs:comment>The pizza ontology that describes various pizzas based on their toppings.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#hasBase -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#hasBase">
        <rdfs:subPropertyOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#isBaseOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#Pizza"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#PizzaBase"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#hasIngredient -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#hasIngredient">
        <owl:inverseOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#isIngredientOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#hasSpiciness -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#hasSpiciness">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#SpicinessValuePartition"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#hasTopping -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#hasTopping">
        <rdfs:subPropertyOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#isToppingOf"/>
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#Pizza"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#PizzaTopping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#isBaseOf -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#isBaseOf">
        <rdfs:subPropertyOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#isIngredientOf -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#isIngredientOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#isToppingOf -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#isToppingOf">
        <rdfs:subPropertyOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#hasCalorificContentValue -->

    <owl:DatatypeProperty rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#hasCalorificContentValue">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#AmericanHotPizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#AmericanHotPizza">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#JalapenoPepperTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#JalapenoPepperTopping"/>
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#MozzarellaTopping"/>
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#PepperoniTopping"/>
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Pizza that only has Mozarella and Tomato toppings</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#AmericanaPizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#AmericanaPizza">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#MozzarellaTopping"/>
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#PepperoniTopping"/>
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Pizza that only has Mozarella and Tomato toppings</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#AnchovyTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#AnchovyTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#SeaFoodTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#CaperTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#CaperTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#VegetableTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#CheeseTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#CheeseTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#PizzaTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#CheesyPizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#CheesyPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#hasTopping"/>
                        <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#CheeseTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#DeepPanBase -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#DeepPanBase">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#PizzaBase"/>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#ThinAndCrispyBase"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#FourCheesePizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#FourCheesePizza">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#hasTopping"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">4</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#CheeseTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#GreenPepperTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#GreenPepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#PepperTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#HamTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#HamTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#MeatTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#HighCaloriePizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#HighCaloriePizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#hasCalorificContentValue"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">400</xsd:minInclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#Hot -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#Hot">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#SpicinessValuePartition"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#InterestingPizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#InterestingPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#hasTopping"/>
                        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">3</owl:minCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#JalapenoPepperTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#JalapenoPepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#PepperTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#hasSpiciness"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#Hot"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#LowCaloriePizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#LowCaloriePizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#hasCalorificContentValue"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:maxExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">400</xsd:maxExclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#MargheritaPizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#MargheritaPizza">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#MozzarellaTopping"/>
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Pizza that only has Mozarella and Tomato toppings</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#MeatTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#MeatTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#PizzaTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#hasSpiciness"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#Hot"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#Medium -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#Medium">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#SpicinessValuePartition"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#Mild -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#Mild">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#SpicinessValuePartition"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#MozzarellaTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#MozzarellaTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#CheeseTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#hasSpiciness"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#Mild"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#ParmezanTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#MushroomTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#MushroomTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#VegetableTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#NamedPizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#NamedPizza"/>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#NonVegetarianPizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#NonVegetarianPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#Pizza"/>
                    <owl:Class>
                        <owl:complementOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#VegetarianPizza"/>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#VegetarianPizza"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#OliveTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#OliveTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#VegetableTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#OnionTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#OnionTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#VegetableTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#ParmezanTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#ParmezanTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#CheeseTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizzapizza.owl#PepperTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizzapizza.owl#PepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizzapizza.owl#VegetableTopping"/>
